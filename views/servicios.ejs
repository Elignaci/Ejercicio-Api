<%- include('templates/header') %>
<%- include('templates/nav') %>
<main>
    <h2> servicios</h2>
    <% if (servicios.length > 0) { %>
        <% servicios.forEach(({descripcion, nombre, disponible}) => { %>

            <% if (disponible) { %>
                <article>
                    <h2><%= nombre %></h2>
                    <p><%= descripcion %></p>
                </article>
                <% } %>
        <% }) %> <% } else { %>
         <p>Servicios no disponibles</p>

    <%}%>
</main>